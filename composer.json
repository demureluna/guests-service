{
    "name": "demureluna/guestsservice",
    "description": "API microservice for CRUD operations on a guest",
    "type": "project",
    "keywords": [
        "mezzio",
        "middleware",
        "microservice"
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "laminas/laminas-component-installer": true
        },
        "platform": {
            "php": "8.1.0"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "composer/package-versions-deprecated": "^1.10.99",
        "davedevelopment/phpmig": "^1.7",
        "giggsey/libphonenumber-for-php": "^8.13",
        "illuminate/database": "^10.48",
        "laminas/laminas-component-installer": "^2.6 || ^3.0",
        "laminas/laminas-config-aggregator": "^1.6",
        "laminas/laminas-diactoros": "^3.0.0",
        "laminas/laminas-servicemanager": "4.0.0",
        "laminas/laminas-stdlib": "^3.6",
        "mezzio/mezzio": "^3.7",
        "mezzio/mezzio-fastroute": "^3.11.0",
        "mezzio/mezzio-helpers": "^5.7",
        "mezzio/mezzio-twigrenderer": "^2.15",
        "symfony/dotenv": "^6.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~2.5.0",
        "laminas/laminas-development-mode": "^3.12.0",
        "mezzio/mezzio-tooling": "^2.9",
        "filp/whoops": "^2.15.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/src/",
            "GuestsService\\": "src/GuestsService/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
